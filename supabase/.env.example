# Supabase Edge Functions Environment Variables
# Copy this to .env and fill in your values

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Stripe Configuration
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_signing_secret

# Stripe Price IDs (create these in Stripe Dashboard)
# Starter Plan
STRIPE_STARTER_MONTHLY_PRICE_ID=price_starter_monthly
STRIPE_STARTER_YEARLY_PRICE_ID=price_starter_yearly
STRIPE_STARTER_LIFETIME_PRICE_ID=price_starter_lifetime

# Pro Plan
STRIPE_PRO_MONTHLY_PRICE_ID=price_pro_monthly
STRIPE_PRO_YEARLY_PRICE_ID=price_pro_yearly
STRIPE_PRO_LIFETIME_PRICE_ID=price_pro_lifetime

# Elite Plan
STRIPE_ELITE_MONTHLY_PRICE_ID=price_elite_monthly
STRIPE_ELITE_YEARLY_PRICE_ID=price_elite_yearly
STRIPE_ELITE_LIFETIME_PRICE_ID=price_elite_lifetime

# Desktop License
STRIPE_DESKTOP_LIFETIME_PRICE_ID=price_desktop_lifetime

# Frontend URL (for redirects)
FRONTEND_URL=http://localhost:5173
